
<%= simple_form_for(@zip_code) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :region %>
    <%= f.input :name %>
    <%= f.input :latitude %>
    <%= f.input :longitude %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
